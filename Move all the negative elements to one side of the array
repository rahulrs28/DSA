import java.io.*;
import java.util.*;
class GFG {
	public static void main (String[] args) {
	    int size=9; 
	    int arr[]= new int[]{ -12, 11, -13, -5, 6, -7, 5, -3, -6}; 
	    
		int len=0;
		for(int i=0;i<size;i++){ 
		if(arr[i]<0){ int temp = arr[i]; arr[i]=arr[len];arr[len]=temp; len++;} }  
// 		for(int i=0;i<size;i++){ 
// 		if(arr[i]>0){ int temp = arr[i]; arr[i]=arr[len];arr[len]=temp; len++;}  } 
 		for(int i=0;i<size;i++)
		System.out.print(arr[i]+" ");
		    
	}
}
