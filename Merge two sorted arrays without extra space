class Solution {

    public void merge(int arr1[], int arr2[], int n, int m) {
       int temp,j=0;
      //corner case if arr size is 1
      if(m!=1 || n!=1){
      for(int i=0;i<n;i++){
          if(arr1[i]>arr2[j]){
              temp=arr1[i];
              arr1[i]=arr2[j];
              arr2[j]=temp;
              Arrays.sort(arr2);
             
          }
          
      }
       
       Arrays.sort(arr1);
       Arrays.sort(arr2);
   
    }}
}
